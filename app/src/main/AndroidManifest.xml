<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="io.github.dearzack.helloandroid">

    <uses-sdk tools:overrideLibrary="moe.codeest.enviews" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".APP"
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.JsoupActivity" />
        <activity android:name=".activity.SQLiteOnWebActivity" />
        <activity android:name=".activity.SmartisanNotesActivity" />
        <activity android:name=".activity.BaseActivity" />
        <activity android:name=".activity.ENViewsActivity" />
        <activity android:name=".activity.NetEasyPlayerActivity" />
        <activity android:name=".activity.IconfontActivity" />
        <activity android:name=".activity.CoordinatorActivity" />
        <activity android:name=".activity.TransitionActivity" />
        <activity android:name=".activity.TouchActivity" />
        <activity android:name=".activity.TransitionAnimationActivity" />
        <activity android:name=".activity.WebActivity"></activity>
    </application>

</manifest>